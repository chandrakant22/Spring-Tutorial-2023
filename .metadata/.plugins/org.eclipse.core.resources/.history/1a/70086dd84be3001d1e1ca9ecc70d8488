
public class Employee {

	private int id;
	private String name;
	private Address address;
	
	
	public Employee(int id, String name, Address address) {
		super();
		this.id = id;
		this.name = name;
		this.address = address;
	}
	
	public Employee(int id) {
		super();
		this.id = id;
	}
	
	public Employee(String name) {
		super();
		this.name = name;
	}
	
	public Employee(Address address) {
		super();
		this.address = address;
	}
	
}
